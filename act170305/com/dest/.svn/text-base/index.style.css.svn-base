.MaskDownload {
  z-index: 9999;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  -webkit-transition: opacity 1s;
  transition: opacity 1s;
}
.MaskDownload .mask-panel {
  overflow: hidden;
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  width: 5.5rem;
  height: 3.6rem;
  margin: auto;
  border-radius: 0.1rem;
  background: white;
}
.MaskDownload .mask-panel .top {
  box-sizing: border-box;
  height: 0.9rem;
  border-bottom: 1px solid #d8d8d8;
  font-size: 0.3rem;
  line-height: 0.9rem;
  font-weight: normal;
  text-align: center;
}
.MaskDownload .mask-panel .middle {
  box-sizing: border-box;
  height: 1.8rem;
  border-bottom: 1px solid #d8d8d8;
  font-size: 0.28rem;
  line-height: 1.8rem;
  text-align: center;
}
.MaskDownload .mask-panel .bottom {
  overflow: hidden;
}
.MaskDownload .mask-panel .bottom li {
  float: left;
  width: 50%;
  height: 0.9rem;
  line-height: 0.9rem;
  font-size: 0.28rem;
  text-align: center;
}
.MaskDownload .mask-panel .bottom .confirm {
