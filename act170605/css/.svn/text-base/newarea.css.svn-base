.books li a.btn{ position: absolute; right: 16px; bottom: 14px; background: #3399ff; height: 24px; line-height: 26px; text-align: center; width: 50px; font-size: 12px;z-index: 10;}
.books li a.btn.disable {background: #ccc;}
.books li a.btn.active{ background: #0d86ff;}
.bookinfo{ padding:16px 16px 0 96px;}
.bookinfo h4{font-size:16px;color:#141414;line-height:16px; margin-bottom: 7px;white-space:nowrap; overflow: hidden; text-overflow: ellipsis;}
.detail_cj{display: -webkit-box;-webkit-line-clamp:2;overflow: hidden;text-overflow: ellipsis;-webkit-box-orient: vertical;font-size: 14px; line-height:20px; height: 40px; margin-bottom: 10px;}
.new_author{ height: 12px;background: url(../images/newperson.png) 0 center no-repeat;-webkit-background-size:12px 12px; line-height: 12px; font-size: 12px; padding-left: 14px; position: relative;}
.morelink{ position: fixed; left: 0; bottom: 0; width: 100%; padding:16px; box-sizing: border-box; z-index: 99; background:#fff;box-shadow: 0px -1px 1px 1px rgba(0, 0, 0, 0.08);}
.morelink .btn{ background: #3399ff;}

.book_list { display: -webkit-box; -webkit-box-sizing: border-box; padding: 14px 16px 8px 16px; -webkit-box-align: top; position: relative; height: 100%;}
.book_list ul{overflow:hidden; position: relative; }
.book_list li { width: 33.33333%;display: block;padding-bottom: 24px;}
.book_list li>div{ display:inline-block; position: relative; margin: 0; padding: 0;}
.book_list li:nth-child(1){ text-align: left;}
.book_list li:nth-child(2){ text-align: center;}
.book_list li:nth-child(3){ text-align: right;}
.book_list img::after{content: ''; position: absolute; left: 1px; top: 1px; width: 84px; height: 112px; background: #000; z-index: 9999;}
.book_list li span { position: absolute; left: 0; top: 0; width: 84px; height: 112px }
.book_list li span.active { background: #000; opacity: .3; position: absolute; left: 1; top: 1; width: 84px; height: 112px;}
.book_list li img { border: 1px solid #f2f2f2; display: block; width: 84px; height: 112px; margin-bottom: 7px;-webkit-box-shadow:0 2px 3px rgba(0, 0, 0, 0.08); }
.book_list li p { font-size: 12px; text-align: left; color: #999; line-height: 16px; display: -webkit-box; -webkit-line-clamp: 1; -webkit-box-sizing: border-box; overflow: hidden; text-overflow: ellipsis; -webkit-box-orient: vertical; -webkit-box-align: top; word-break: break-all; width: 85px }
.book_list li p.book_name { font-size: 14px; color: #141414; -webkit-line-clamp: 2; line-height: 18px;box-orient: vertical;}
.book_list li p.bookname { font-size: 14px; color: #141414; -webkit-line-clamp: 2; line-height: 18px}
.book_list li p.p_del{ text-decoration: line-through; color: #999;}
.book_list li p.p_red{color: #ff5959;}
.book_list li .btn{ background: #3399ff; position: absolute; width:74px ; height: 24px; color: #fff;line-height: 26px; font-size: 12px;  bottom: 8px;}
.book_list li .btn.active{ background: #0d86ff;}
.book_list li:nth-child(1) .btn{left: 22px;}
.book_list li:nth-child(2) .btn{left: 50%; margin-left: -37px;}
.book_list li:nth-child(3) .btn{right: 22px;}

/*10天限免*/
.banner{ position: relative;}
.banner p{ position: absolute; bottom:0.72rem; color: #fff; font-size: 0.24rem; text-align: center;}
.banner img{ display: block; width: 100%;}
.countDown{ width: 3.5rem; top: 0; padding-top: 0.2rem; color: #fff; vertical-align: bottom; font-size: 0.24rem;}
.countDown span#countDownDay,.countDown span#countDownHour,.countDown span#countDownMin{ width: 0.6rem; height: 0.72rem; display: inline-block;background: url(../images/numBg.png) no-repeat;-webkit-background-size:cover; line-height: 0.76rem; font-size: 0.44rem; text-align: center; color: #3399ff; margin: 0 0.12rem; opacity: 1; font-weight: bold;}
.countDown span{width: 0.6rem; height: 0.72rem; vertical-align: bottom; opacity: 0.6;}
.iOS .countDown span#countDownDay,.iOS .countDown span#countDownHour,.iOS .countDown span#countDownMin{ line-height: 0.72rem;}
