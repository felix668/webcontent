/*css reset */
body, div, p, ul, ol, li, textarea, form, input, h1, h2, h3, h4, h5, dl, dt, dd, img, em { margin: 0; padding: 0; color: #999; font-size: 14px }
em, i, b { font-style: normal; font-weight: normal }
h1, h2, h3, h4, h5 { font-weight: normal }
body { font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif; -webkit-text-size-adjust: none; background: #fff }
ul, ol, li { list-style: none }
img { border: 0 none; -webkit-touch-callout: none }
a { text-decoration: none; color: #141414; -webkit-touch-callout: none; -webkit-user-select: none }
html, body { -webkit-user-select: none; user-select: none; height: 100% }
* { -webkit-tap-highlight-color: rgba(0,0,0,0) }
.active { background-color: #f2f2f2 }
.none { display: none }
.auto-x{position: absolute;left: 0;right: 0;margin: auto}
.auto-y{position: absolute;top: 0;bottom: 0;margin: auto}
.tl{ text-align:center;}

/*头部*/
.newbg{ background-color:#f0f1f2;}
.head{ position:relative; width:100%; height:7.6rem; margin-bottom:12px;}
.head img{ width:100%; display:block;}
.slogan{ width:3.02rem; height:0.55rem; position:absolute; left:0.24rem; top:0.24rem; z-index:99;}
.person{ width:100%; height:5.3rem; position:absolute; left:0; top:0;z-index:10;}
.mo{ width:100%; height:3.62rem; position:absolute; left:0; top:3.92rem;z-index:99;}
.newbtn,.newbtn.active{bottom:0.5rem; height:0.7rem; padding-top:0.1rem; width:5.2rem; text-align:center;z-index:1000;background:url(../images/newbtn.png) no-repeat; -webkit-background-size:5.2rem 0.8rem; font-size:0.26rem; color:#fff;}
.newbtn span{ font-size:0.44rem;}
.newbtn.active{background:url(../images/newbtn_pr.png) no-repeat; -webkit-background-size:5.2rem 0.8rem;}

.pername{ font-size:16px; color:#fff; position:absolute; right:1.7rem; top:0.76rem; display:block; padding:0 0.24rem; font-size:0.26rem; color:#fff;}
.pername:before{content: ''; position: absolute; left:0; bottom:0; width:0.24rem; height:0.18rem; background:url(../images/namebg.png) 0 bottom no-repeat; -webkit-background-size:6rem 0.18rem; z-index:0}
.pername:after{content: ''; position: absolute; right:0; bottom:0; width:0.24rem; height:0.18rem;background:url(../images/namebg.png) right bottom no-repeat; -webkit-background-size:6rem 0.18rem; z-index:0}
.names{ position:relative; z-index:9999}
.namebg{ position:absolute; height:0.18rem; left:0.22rem; right:0.22rem; bottom:0; background-color:#9c1f23;}
/*领书券*/
.quanstep{ position:relative;}
.quanstep ul,.quanBtn{display: -webkit-box; padding:0 0.375rem;}
.quanstep ul li{-webkit-box-flex: 1; text-align:center; position:relative; height:2.2rem; z-index:9;}
.quanstep ul li p{ font-size:14px; color:#9c1f23;}
.quanstep ul li:first-child p{ position:absolute; top:0.38rem; left:0; right:0;}
.quanstep ul li:nth-child(2) p{ position:absolute; top:0.28rem; left:0; right:0;}
.quanstep ul li:last-child p{ position:absolute; top:0.13rem; left:0; right:0;}
.quanstep b{ font-size:16px;}
.quanstep span{ display:inline-block; width:1.4rem; height:1.65rem; background:url(../images/readStep.png) no-repeat;-webkit-background-size:10rem 1.65rem; position:absolute; bottom:0; left:50%; margin-left:-0.7rem;}
.quanstep span.step1{ background-position:0 bottom; height:1.4rem;}
.quanstep span.step1s{ background-position:-1.5rem bottom ; height:1.4rem;}
.quanstep span.step2{  background-position:-3rem bottom; height:1.58rem;}
.quanstep span.step2s{  background-position:-4.5rem bottom; height:1.58rem;}
.quanstep span.step3{  background-position:-6rem bottom; height:1.68rem;}
.quanstep span.step3s{  background-position:-7.5rem bottom; height:1.68rem;}
.stepbg{ width:6.3rem; height:0.6rem; background-color:#e3dada;transform: skew(-30deg, 0deg); position:absolute; right:0; left:0; margin:0 auto; bottom:-0.18rem; z-index:0;}
.quanline{ width:0.58rem; height:2px;-webkit-border-radius:2px; background-color:#9c1f23; position:absolute; top:1.6rem;}
.quanline:before{content: ''; position: absolute; left:50%; top:50%; margin-left:-3px; margin-top:-3px; width:6px; height:6px;-webkit-border-radius:6px; z-index:9;background-color:#9c1f23;}
.quanline.lineL{ left:2.25rem;}
.quanline.lineR{ right:2.25rem;}
.quanp{ color:#b28f90;}
.quanBtn{ overflow:hidden; margin-top:0.42rem;}
.quanBtn li{-webkit-box-flex: 1; text-align:center;}
.quanBtn li a{ width:1.6rem; height:30px; line-height:32px;-webkit-border-radius:0.6rem; display:inline-block; color:#fff; font-size:14px;}
.btndis{ background-color:#cccccc}
.btngo{ background-color:#9c1f23}
.btngo.active{ background-color:#73171a;}
.btnwait{ background-color:#cc9798}

/*标题*/
.newtitle{ text-align:center; overflow:hidden; padding-top: 16px;}
.newtitle p{ text-align:center; display:inline-block; font-size:16px; color:#ff5959; padding:0 102px 0 20px;-webkit-border-radius: 24px;height:24px; line-height: 24px; position:relative;}
.newtitle p.titleTime{ padding:0 98px 0 26px; background-color:#ece6e7}
.newtitle p span{ display:block; position:absolute; right:26px; top:50%; margin-top:-8px; width:64px; height:16px; line-height:17px; font-size:12px; color:#fff; padding:0 4px; background-color:#ff5959;-webkit-border-radius: 14px;}
.newtitle p:before,.newtitle p:after{ content: ''; position: absolute; top:8px; width:28px; height:9px;background:url(../images/newpoint.png) no-repeat;-webkit-background-size:28px 9px;-webkit-border-radius:10px;}
.newtitle p:before { left: -12px;-webkit-transform:scaleX(-1);}
.newtitle p:after {right: -12px;}
.newtitle p.pTitle{padding: 4px 20px; line-height: 16px;}
/* 大书封列表 */
.booklayout{ position:relative; padding:0 0 6px 0;}
.booklist { display: -webkit-box; -webkit-box-sizing: border-box; padding:0 0.3rem 0.2rem 0.3rem; -webkit-box-align: top; position: relative;overflow:hidden; }
.booklist li { -webkit-box-flex: 1; width:100%; margin: 0 auto; position: relative; display:block; overflow:hidden; }
.booklist li div{ margin:0 auto;  width:1.68rem; position:relative}
.booklist li:nth-child(1)>div{float:left;}
.booklist li:nth-child(3)>div{float:right;}
.booklist li div.bookface{ width:1.68rem; height:2.24rem; position:relative; margin-bottom:5px}
.booklist li img {display: block; width:1.68rem; height:2.24rem;-webkit-box-shadow:0 2px 3px rgba(0, 0, 0, 0.08);box-shadow:0 2px 3px rgba(0, 0, 0, 0.08);}
.booklist li .bookface .oclick{width:1.68rem; height:2.24rem; display: block; position: absolute; left: 0; top: 0;z-index: 9;}
.booklist li .bookface .oclick.active{background: #000;opacity: .3;}
.booknum{ width:0.6rem; height:0.3rem; display:block; position:absolute; right:0; bottom:0;background:rgba(0,0,0,0.6); line-height:0.3rem; text-align:center; font-size:12px; color:#ff9933;}
.booklist li p { font-size:11px;}
.booklist li p.book_name { font-size:14px; color: #141414; display: -webkit-box;-webkit-line-clamp:2;overflow: hidden;text-overflow: ellipsis;-webkit-box-orient: vertical; line-height: 18px;line-height: 17px;}
.bookauthor{color:#999; font-size:11px;line-height:1em; margin-top:4px;}
.forIntro{ color:#999; font-size:14px; line-height:1em; margin:12px 16px 16px 16px; text-align: center;}
.bookface .active{ display:none;}
/*.active .active{width: 1.68rem; height: 2.24rem; position:absolute; left:0; top:0;background:rgba(0,0,0,0.2); display:block;}*/
/* 还能做列表 */
.class_list{ width:100%;}
.book_list2 { margin:20px 16px 0;height: 90px; padding-left:80px;margin-bottom: 15px; position: relative; display: -webkit-box; -webkit-box-sizing: border-box; -webkit-box-align: center; line-height:1em;}
.book_list2 img { position: absolute; left: 0; top: 0; width:68px; height:91px }
.class_name{font-size:16px; color: #141414; margin-bottom:15px; line-height:17px; white-space:nowrap; overflow: hidden; text-overflow: ellipsis;}
.infos{font-size:14px; color: #999; margin-top:12px;}
.btns{ padding:0 8px; overflow:hidden; display:-webkit-box;width:100%;box-sizing: border-box; z-index:99;}
.btns a{ -webkit-box-flex:1; display:block; line-height:1em; background:#9c1f23; padding:7px 0; margin:0 8px; text-align:center; color:#fff; font-size:16px;-webkit-border-radius:5px;}
.btns a.active{ background-color:#73171a;color:#fff;}
/* 评论部分*/
.commentlayout{ margin-bottom:40px}
.comment { margin-top:20px;background: -webkit-gradient(linear, left bottom, left top, color-stop(.5, transparent), color-stop(.5, #e3e4e5), to(#e3e4e5)) left top repeat-x;background-size: 100% 1px;}
.comment .comBor{ padding:0 16px;}
.comment dl {display: -webkit-box; padding: 12px 0 9px 0;background: -webkit-gradient(linear, left top, left bottom, color-stop(.5, transparent), color-stop(.5, #e3e4e5), to(#e3e4e5)) left bottom repeat-x;background-size: 100% 1px;}
.comment dt { width: 32px }
.comment dd { -webkit-box-flex: 1; position: relative }
.comment dd h4 { color: #141414; display: -webkit-box;overflow: hidden;text-overflow: ellipsis;-webkit-box-orient: vertical;-webkit-line-clamp:2; line-height:20px; font-weight:normal; margin-bottom:2px;}
.comment .face { width: 32px; height: 42px; position: relative; margin: 0 auto 8px; position: relative }
.comment .face img { width: 24px; height: 24px; border-radius: 50px }
.userinfo { position: relative; padding-top:4px; height: 23px }
.userinfo h3 { font-size: 14px; height: 13px; line-height: 15px; margin-bottom: 4px }
.userinfo p { font-size: 12px; color: #999 }
.detail_p{ font-size: 14px; overflow: hidden; color: #141414; line-height:21px; color: #666; text-align:justify;display: -webkit-box;overflow: hidden;text-overflow: ellipsis;-webkit-box-orient: vertical;}
.clamp4{-webkit-line-clamp:4;}
.clamp5{-webkit-line-clamp:5;}
.username { color: #99664c; margin-right:4px;}
.pk .username { color: #99664c; margin: 0 4px  0 0; }
.authorPart .username { margin-left: 0 }

.book_score{height:14px;margin-bottom:7px; margin-top:2px;}
.book_score em{float:left; width:12px; height:12px; margin-right:5px; display:inline-block}
.book_score i{ margin-left:3px; color:#666;}
.start_1,.start_2,.start_3{ background:url(../images/star.png) no-repeat;-webkit-background-size:45px 12px;}
.start_1{ background-position:0 0;}
.start_2{ background-position:-15px 0;}
.start_3{ background-position:-30px 0;}


/* 评论部分小图标*/
.icons { position: absolute; display: inline-block; text-align: left; margin-left: 2px; margin-top:-2px; height: 14px;}
.icons span, .userinfo h3 .icons span {width: 17px; height: 14px; display: inline-block; padding: 0; margin: 0; margin-right: 3px;}
.icons span.admin1{ background:url(../images/admin_1.png) no-repeat; -webkit-background-size:17px 14px;}
.icons span.admin2{ background:url(../images/admin_2.png) no-repeat; -webkit-background-size:17px 14px;}
.icons span.auth{ background:url(../images/administrator_icon.png) 0 1px no-repeat; -webkit-background-size:12px 12px;}
.userinfo h3 .icons span.writer,.faceinfo h3 .icons span.writer { width:auto;background: #6291d9 url(../images/usericon.gif) no-repeat 2px 1px; -webkit-background-size: auto 9px; height: 12px; -webkit-border-radius: 2px; font-size: 10px; line-height: 13px; padding: 0 2px 0 11px; color: #fff; margin-right: 3px; overflow:hidden; margin-top:2px;}
.userinfo h3 .icons span.pos,.faceinfo h3 .icons span.pos, .userinfo h3 .icons span.pos_pyq { display: inline-block; -webkit-border-radius: 1px; color: #fff; line-height: 14px; width: 24px; height: 12px; font-size: 10px; text-align: center; /*position: absolute;*/ overflow:hidden;}
.userinfo h3 .icons span.pos_pyq,.faceinfo h3 .icons span.pos_pyq { width: 24px; height: 12px; font-size: 10px; line-height: 13px; -webkit-border-radius: 1px; color: #fff; overflow:hidden;text-align: center; margin-top:2px;}
.pos0, .pos1, .pos-1 { background: #a1bde6 }
.pos10 { background: #ff5959 }
.pos9,.pos8, .pos7, .pos6, .pos5, .pos4 { background: #3399ff }
.posa { background: #6291d9 }
.pos2, .pos3 { background: #91cc3d }
/* 评论信息*/
.post_para{line-height:1em; padding:12px 0 0 0; color:#b2b2b2; font-size:10px;}
.post_para .fl{ float:left;font-size:10px;}
.post_para .fr span{font-size:10px; margin:0 4px;}
.fr{ float:right; font-size:10px;}
.fr p:first-child{ margin-right:6px; width:60px; }
.fr p:last-child{ margin-right:0; width:55px; }
.fr p{display:inline-block; font-size:10px;}
.donum{ margin-left:4px;}

.goreply{ height:45px; text-align:center;background: -webkit-gradient(linear, left top, left bottom, color-stop(.5, transparent), color-stop(.5, #e3e4e5), to(#e3e4e5)) left bottom repeat-x;background-size: 100% 1px;}
.goreply.active{background-color: #e1e2e3 }
.goreply span{ display:inline-block; padding-right:12px; height:14px; line-height:1em; margin-top:16px; background:url(../images/redpoint.png) right top no-repeat; -webkit-background-size:6px auto;color: #9c1f23;}

/* 推荐模块*/
.introlayout{ padding-bottom:20px;}
.sort{text-align:center; padding:4px 30px 4px 30px; font-size:16px;color: #ff5959; display:inline-block; position:relative; margin:0 auto;}
.sort:before {content: ' ';position: absolute; top:0; left:0;width:14px;height:25px;background:url(../images/sort.png) 0 0 no-repeat; -webkit-background-size:88px 25px;}
.sort:after {content: ' ';position: absolute; top:0; right:0;width:14px;height:25px;background:url(../images/sort.png) right 0 no-repeat; -webkit-background-size:88px 25px;}
.introinfo{ line-height:20px; text-align:center; padding:18px 16px 0 16px; color:#666;}
.introinfo span{color: #FF5959;}
.sortinfo{ position:relative; margin:0 0.32rem; width: 6.56rem; height: 3.12rem;}
.sortinfo img,.characterimg img{ display:block; width:100%;}
.sk{background:url(../images/sk.png) no-repeat;}
.ss{background:url(../images/ss.png) no-repeat; }
.dss{background:url(../images/dss.png) no-repeat;}
.mfzq{background:url(../images/mfzq.png) no-repeat;}
.phb{background:url(../images/phb.png) no-repeat;}
.byzq{background:url(../images/byzq.png) no-repeat;}
.iOS .sk{background:url(../images/sk-ios.png) no-repeat;}
.iOS .ss{background:url(../images/ss-ios.png) no-repeat; }
.iOS .dss{background:url(../images/dss-ios.png) no-repeat;}
.iOS .mfzq{background:url(../images/mfzq-ios.png) no-repeat;}
.iOS .phb{background:url(../images/phb-ios.png) no-repeat;}
.iOS .byzq{background:url(../images/byzq-ios.png) no-repeat;}
.sk,.ss,.dss,.mfzq,.phb,.byzq,.iOS .sk,.iOS .ss,.iOS .dss,.iOS .mfzq,.iOS .phb,.iOS .byzq{ -webkit-background-size:100% 100%;}

.btn {display: block; line-height:40px; height:40px; overflow: hidden; text-align: center; color: #fff; -webkit-border-radius: 2px; font-size: 16px }
.doings { padding:10px 0 40px 0;}
.doings .btn { margin: 0 16px}
.doings .btn.bopen { background: #3399ff }
.doings .btn.bopen.active { background: #0d86ff}

.characterimg{ position:relative; padding-bottom:8px; margin:20px 1.25rem 0 1.88rem;}
.characterimg .uesrface{ width:1.08rem; height:1.08rem; border:1px solid #9c1f23; position:absolute; left:1.17rem; top:0.98rem;-webkit-border-radius:1.1rem; background-color:#e5e5e5;}
.characterimg .uesrface img{width:1.08rem; height:1.08rem;-webkit-border-radius:1.1rem;}

.characterinfo,.characterinfos{line-height:20px; text-align:center; padding:10px 16px 18px 16px;}
.characterinfos{padding:15px 16px 0 16px;}
.doings .btn.dis { background: #ccc }
.ctc{ text-align:center;}
/* 跟随加载 */
.loading.loadingTop{ padding:0; position:absolute; top:50%; margin-top:-20px;display:block; width:100%; text-align:center;}
/* 居中正在加载 */
.loadingTop{ display:block; margin:0 auto; text-align:center; padding:5px 0;}
.loadingTop .loads {display:block; position:absolute; left:0; top:0; font-size: 0;}
@-webkit-keyframes rotate{
	0%{-webkit-transform:rotate(0deg)}
	100%{-webkit-transform:rotate(360deg);}
}
.loadtc,.loadall{display:inline-block; line-height:42px; height:40px; position:relative; padding-left:40px; color:#999; font-size:14px;}
.loadall{ padding-left:0; }/*已显示全部*/
.loads{ width:24px; height:24px; padding: 8px; -webkit-animation:rotate 1s linear infinite;display:block; margin:0 auto}
/*加载失败*/
.load_fail{color:#999; text-align:center; line-height:60px; font-size:14px; padding-bottom:20px;}
.load_fail a{font-size:16px; color:#fff; text-align:center; background:#3399ff; display:block; margin:0 auto; width:210px; height:35px; line-height:35px; border-radius:3px;}
.load_fail a.active{ background:#0d86ff;}

/* 阅读时长*/
.exchange{ padding-bottom:39px;padding-top: 20px;}
.echange{background-color: #3399ff; position: relative;}
.echange>img{ display: block; width: 100%;}
.states{ text-align:center; padding:0 0 20px 0;}
.exchange .states{ text-align:center; padding:0 0 20px 0;position: absolute;left: 0;right: 0;margin: auto; top: 40px;}
.states img{ width:53px; height:53px;-webkit-border-radius: 53px;}
.readchange{ margin:0 16px; font-size:16px; color:#333; text-align:center; padding-top:46px; position: relative;}
.readchange p{ line-height:1em; font-size:12px; color:#fff;}
.readchange p.reads{ font-size:14px; color:#fff; margin-bottom:6px;}
.reads span{ margin:0 2px;}
.readBtn{ margin:14px 16px 0 16px; }
.readBtn a{ height:40px; line-height:40px; text-align:center; display:block; color:#fff; font-size:16px; background-color:#3399ff;-webkit-border-radius:2px; margin-bottom:24px;}
.readBtn a.active{ background-color:#0d86ff;}
.readBtn a.disable{background-color:#ccc;color:#fff;}
.readchange span{ color:#fff;}
.readtimes{ padding-top: 86px;}
.readinfo{ margin:0 16px; height: 182px; background:rgba(51,153,255,.8) url(../images/rtime.png) no-repeat left bottom;-webkit-background-size:100% auto; position: relative; border-radius: 3px; text-align: center;}

.exchangeList .newtitle p,.authorPart .newtitle p{color:#3399ff; border:1px solid #3399ff;background-color: #fff;}
.exchangeList .newtitle p span,.authorPart .newtitle p span{color:#3399ff; background-color:#3399ff;}
.exchangeList .newtl,.exchangeList .newtr,.authorPart .newtl,.authorPart .newtr{background-color:#3399ff;}
.exchangeList .newtitle p:before,.exchangeList .newtitle p:after,.authorPart .newtitle p:before,.authorPart .newtitle p:after{background: #3399ff;}

.explain{ width:100%; left:0; top:4.2rem; line-height:24px; font-size:16px; color:#333; text-align:center;}
.explain p{ color:#999;}
.explain span{ color:#3399ff;}

.introbook{ padding-top:6px;}
.bag li {padding: 0; border: 0; display: block;border-bottom: 1px solid #f2f2f2 }
.bag li:last-child{border:none;}
.bag li a { display: block; height: 67px; padding: 12px 0; margin: 0 16px; position:relative;}
.bagCover { width: 50px; height: 67px; position: absolute; left: 0; top: 12px }
.bagCover img { width: 50px; height: 67px }
.bagDetail { -webkit-align-items: center; display: -webkit-box; -webkit-line-clamp: 3; overflow: hidden; text-overflow: ellipsis; -webkit-box-orient: vertical; width: 100%; padding-left: 62px; padding-top: 1px; -webkit-box-sizing: border-box }
.bagDetail h3 { font-size: 16px; text-align: left; text-align: left; position: relative; color: #333; margin-bottom:6px;white-space:nowrap; overflow: hidden; text-overflow: ellipsis;}
.bagDetail p {font-size: 14px; color: #999; line-height: 20px; height: 20px; position: relative;white-space:nowrap; overflow: hidden; text-overflow: ellipsis;}

.states.logins{padding:0; font-size:0; position: absolute; left:50%; margin-left: -34px; top: 0; margin-top: -38px;}
.states p{ color:#99664b; margin-top:4px;}
.states .userpic{ width:68px; height:68px; border:2px solid #f2f2f2;-webkit-border-radius:68px; display:inline-block; font-size:0; position:relative}
.states .userpic img{width:68px; height:68px;-webkit-border-radius: 68px;}
.canchange{display: -webkit-box;-webkit-box-align: center;-webkit-box-pack:center;}
.canchange p{font-size:20px; color:#141414;}
.dochange{ text-align: center;  position:absolute; left: 0; right: 0; bottom: 50px;}
.dochange p{ display:inline-block;font-size:19px; color:#fff; padding-left:30px;background:url(../images/coin.png) no-repeat; background-size:24px 24px; height:24px;line-height:24px;}
.popul{ float:right; color:#8e5539;}
.readchange p.doend{ font-size:20px; color:#333;  height:24px; line-height:24px; margin-bottom:29px;}
.footbtn{ position:fixed; width:100%; left:0; right:0; line-height:20px; bottom:0; text-align:center; font-size:14px; color:#999; padding-bottom:20px;}
.footbtn p{ padding:0 16px;}
/* 更多畅销书*/
.doings { padding:5px 0;}
.addTopline{ border-top:1px solid #f2f2f2;}
.doings.addTopline{ padding:10px 0  20px 0;}
.doings.addTopline .btn { margin: 0 14px 0 14px }
.doings.addTopline .btn.bdown { background: #3399ff }
.doings.addTopline .btn.bdown.active { background: #0d86ff}
.doings.addTopline p { font-size: 14px; color: #999; text-align: center; padding: 0 14px }

/* 感谢信*/
.gxxhead{ position:relative; width:100%; margin-bottom:20px;}
.gxxhead a{width:2.1rem; height:2.0rem; display:block;  position:absolute; top:1.4rem; right:0.65rem; z-index:30;}
.gxxhead a.active{ background:none;}
