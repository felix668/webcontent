/*wrap*/
.wrap{opacity: 0;position:absolute;width:100%;top:0;bottom:0;left:0;overflow:scroll;-webkit-overflow-scrolling:touch;z-index:0;display: none;}
.show{-webkit-animation:fadeIn 1s;z-index: 10;display: block;}
@-webkit-keyframes fadeIn{
	100%{opacity: 1}
}
.close{position:absolute;right:.16rem;top:.56rem;width: .5rem;height: .5rem;background: url(../images/close.png);background-size: 100% auto;}
/*视频*/
.vbox{padding-top:1.28rem;position: relative;width: 7.08rem;margin: 0 auto;}
.playv{position: absolute;width: 6.9rem;height: 4.02rem;top:4.66rem;left:.1rem;}
.vmask{position: fixed;width: 100%;height: 100%;top:0;left: 0;background: #222;z-index: 20;-webkit-box-align:center;-webkit-box-pack:center;display: none;}
.vani{-webkit-animation:slideLeft .6s;display:-webkit-box;}
@-webkit-keyframes slideLeft{
	0%{-webkit-transform:translate3d(-100%,0,0);}
}
.varea{position:relative;width:6.9rem;height:4rem;background: url(../images/border.png) no-repeat;background-size: 100% 100%;padding:.04rem;-webkit-box-sizing:border-box;}
.loading{position: absolute;top:0;bottom:0;left:0;right:0;margin:auto;width: .32rem;}
.varea video{width:100%;height: 100%;display: block;object-fit:fill;}
video::-webkit-media-controls {
  display:none !important;
}
/*海报*/
.poster{margin-top:.96rem;}
.swiper-small{width: 100%;height: 2.04rem;overflow:hidden;margin:.46rem 0 .78rem;padding:0 .1rem;background: rgba(90,129,184,.15);-webkit-box-sizing:border-box;box-sizing:border-box;}
.swiper-wrapper{width:100%;height:100%;display: -webkit-box;-webkit-box-align:center;-webkit-box-pack:center;display: -webkit-flex;display: flex;-webkit-content-justify:center;-webkit-align-items:center;-webkit-transition-property: -webkit-transform; -webkit-transform: translate3d(0,0,0);}
.swiper-slide {-webkit-flex-shrink: 0;flex-shrink: 0;}
.swiper-small .swiper-slide{width:3.24rem;height:1.64rem;margin:0 .1rem;background: url(../images/border.png) no-repeat;background-size:100% 100%;display: -webkit-box;-webkit-box-align:center;-webkit-box-pack:center;}
.swiper-small .swiper-slide img{width:3.2rem;height:1.6rem;}
.swiper-big{position:fixed;top:0;left:0;width: 100%;height: 100%;overflow:hidden;background: #222;z-index: -1;opacity: 0;-webkit-transition:opacity .3s .2s;}
.swiper-big .swiper-slide{width:100%;}
.swiper-pagination {
    position: absolute;
    width:100%;
    bottom: .4rem;
    text-align: center;
    -webkit-transition: .3s;
    -webkit-transform: translate3d(0,0,0);
    z-index: 20;
}
.swiper-pagination-bullet {
    width: .12rem;
    height: .12rem;
    display: inline-block;
    border-radius: 50%;
    background: #666;
    margin:0 .1rem;
}
.swiper-pagination-bullet-active {
    background: #eee;
}
.share{width: 5.28rem;margin:0 auto;-webkit-transition:all .5s;}
.share:active{-webkit-transform:scale(.9);}
/*大学生入口*/
.college{height: .78rem;display:inline-block;margin:.3rem 0 .7rem 2.6rem;font-size: .28rem;color: #cdcdcd;line-height: .78rem;padding-left: .6rem;background: url(../images/icon_people.png) left center no-repeat;background-size: .37rem auto;}
.maskForm {position: fixed; left: 0; top: 0;width: 100%; height: 100%;z-index: 9999;background: rgba(0,0,0,0.5);overflow: auto;-webkit-overflow-scrolling:touch;display: none;opacity: 0;-webkit-transition:opacity .2s;}
.showForm{display: block;opacity: 1;}
.mask-panel {position: relative;width: 6.28rem;margin: auto;margin-top: 0.5rem;margin-bottom: 4rem;padding-top: 0.64rem;padding-bottom: 0.45rem;border-radius: 0.1rem;background: white;}
.mask-panel .close{right: .15rem; top: .15rem;}
.title {box-sizing: border-box;width: 100%;padding: 0 .68rem;margin-bottom: .35rem;font-size: .32rem; line-height: .46rem;color: #ad6a47; font-weight: 600;text-align: center;}
.info {margin-bottom: .18rem;padding: 0 .68rem;font-size: .24rem;overflow: hidden;}
.left_ {float: left;color: #f14646;}
.right_ {float: right;color: #999999;}
.before input {display: block;-webkit-appearance:none;-webkit-box-sizing: border-box;width: 5.04rem; height: .85rem;margin:0 auto .15rem;border-radius: .08rem;border: 2px solid #d39f76;font-size: .28rem;text-indent: .28rem;outline: none;color: #ad6a47;}
.before input.error {border: 2px solid #f14646;}
.before .submit,.after .edit {width: 3.46rem; height: .84rem;margin: auto; margin-top: .22rem;line-height: .84rem;border-radius: .08rem;background: #bc865c;font-size: .29rem;text-align: center;color: white;}	
.after .field{width: 5.04rem; height: .85rem;margin: auto; margin-bottom: .15rem;line-height: .85rem;font-size: .28rem;text-indent: .28rem;color: #ad6a47;}
.after .key_part {display: inline-block;width: 1.5rem;}
.slide-up-enter-active,.slide-up-leave-active {
  -webkit-transition:all 200ms;
}
.slide-up-enter,.slide-up-leave-to {
  -webkit-transform: translate3d(0, 2%, 0);transform: translate3d(0, 2%, 0);opacity: 0;
}



	