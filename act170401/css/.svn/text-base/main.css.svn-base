body,p,ul,li{margin:0;padding:0;}
ul,li{list-style:none}
img{border:none;width:100%;display: block;}
a{text-decoration:none;-webkit-touch-callout:none;}
*{-webkit-tap-highlight-color:rgba(0, 0, 0, 0);-webkit-animation-fill-mode:both!important}
body{font-family: "microsoft yahei","Helvetica Neue", Helvetica, STHeiTi, sans-serif;-webkit-text-size-adjust:none;-webkit-user-select:none;background: #fff}
/*动画*/
.bg{position:fixed;top:0;left:0;width: 100%;height: 100%;-webkit-animation:slideDown 1.48s 1.04s linear both;}
@-webkit-keyframes slideDown{
	0%{-webkit-transform:translate3d(0,-100%,0);}
	100%{-webkit-transform:translate3d(0,0,0);}
}
.booklist{position:fixed;top:0;left:0;width: 100%;height: 100%;overflow: hidden;}
.booklist img{position: absolute;top:0;left:0;-webkit-animation-fill-mode:both!important;-webkit-animation-timing-function:linear!important;}
.booklist img:nth-child(1){
	width:1.03rem;
	-webkit-animation:drop1 2.08s .48s;
}
@-webkit-keyframes drop1{
	0%{-webkit-transform:translate3d(.69rem,-1.8rem,0) scale(.6) rotate(0);}
	100%{-webkit-transform:translate3d(7.65rem,14rem,0) scale(1) rotate(120deg);}
